  "description": "The website for OpenLineage.io",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn run build:docs && gatsby build",
    "build:docs": "redoc-cli build --output static/spec/openapi.html static/spec/OpenLineage.yml --title 'OpenLineage API Docs'",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
